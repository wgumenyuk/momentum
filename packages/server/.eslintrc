  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    // Semicolon
    "semi": [
      "error",
      "always"
    ],

    // Doppelte Anf√ºhrungszeichen
    "quotes": [
      "error",
      "double"
    ]
  }
}